Tab2:Button("TP Zepter X",function()

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Zepter X", "BloodTheme")

local Tab = Window:NewTab("Players")

local Section = Tab:NewSection("Teleport")

--------------------

players = {}

for i,v in pairs(game:GetService("Players"):GetChildren()) do

   table.insert(players,v.Name)

end

Section:NewDropdown("Select Player", "Select PPL To TP", players, function(abc)

    Select = abc

end)

Section:NewButton("Refresh", "Refresh List", function()

    table.clear(players)

for i,v in pairs(game:GetService("Players"):GetChildren()) do

   table.insert(players,v.Name)

end

end)

Section:NewButton("Teleport", "TP To Selected Plr", function()

    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[Select].Character.HumanoidRootPart.CFrame

end)

end)

Tab2:Button("ESP",function()

while wait() do

   pcall(function()

      for i,v in pairs(game.Players:GetChildren()) do

      if not v.Character.Head:FindFirstChild("ESP") then

       local VIP_ESP = Instance.new("BillboardGui")

            local VIP_TextLabel = Instance.new("TextLabel")

              VIP_ESP.Parent = v.Character.Head

        VIP_ESP.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

                VIP_ESP.Active = true

             VIP_ESP.Name = "ESP"

          VIP_ESP.AlwaysOnTop = true

            VIP_ESP.LightInfluence = 1.000

                VIP_ESP.Size = UDim2.new(0, 200, 0, 50)

               VIP_ESP.StudsOffset = Vector3.new(0, 2.5, 0)

              VIP_TextLabel.Parent = VIP_ESP

                VIP_TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)

           VIP_TextLabel.BackgroundTransparency = 1.000

                VIP_TextLabel.Size = UDim2.new(0, 200, 0, 50)

        VIP_TextLabel.Font = Enum.Font.GothamBold

                VIP_TextLabel.Text = v.Name

          VIP_TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)

                VIP_TextLabel.TextScaled = true

       VIP_TextLabel.TextSize = 14.000

                VIP_TextLabel.TextStrokeTransparency = 0.000

          VIP_TextLabel.TextWrapped = true

            end

        end

    end) 

end

end)

Tab2:Button("M249(For Ambush Anti Cheat)",function()

for i,v in next,game.Players.LocalPlayer:GetDescendants() do

    if v.Name == "ItemShop_M249" then

        v.Visible = true

    end

end

end)

